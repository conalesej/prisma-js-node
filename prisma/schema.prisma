generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          String  @id @default(uuid())
  name        String
  email       String
  isAdmin     Boolean
  preferences Json
  blob        Bytes // Unsuported("") 
  posts       Post[]

  // id          Int     @id @default(autoincrement())
  // largeNumber BigInt
}

model Post {
  id        String   @id @default(uuid())
  author    User     @relation(fields: [authorId], references: [id])
  authorId  String
  rating    Decimal // Float
  createdAt DateTime
  updatedAt DateTime
}
